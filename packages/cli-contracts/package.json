{
  "name": "@kb-labs/cli-contracts",
  "version": "0.1.6",
  "description": "Type definitions and contracts for KB Labs CLI framework (Commands, Context, Presenters, Profiles)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./command": {
      "types": "./dist/command/index.d.ts",
      "import": "./dist/command/index.js",
      "require": "./dist/command/index.js"
    },
    "./context": {
      "types": "./dist/context/index.d.ts",
      "import": "./dist/context/index.js",
      "require": "./dist/context/index.js"
    },
    "./presenter": {
      "types": "./dist/presenter/index.d.ts",
      "import": "./dist/presenter/index.js",
      "require": "./dist/presenter/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --fix --ignore-pattern 'dist/**'",
    "test": "vitest run -c ../../vitest.config.ts",
    "test:watch": "vitest -c ../../vitest.config.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "private": true
}
