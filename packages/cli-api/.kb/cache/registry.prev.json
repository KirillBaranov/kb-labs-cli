                  "name": "exclude",
                  "type": "string",
                  "description": "Types to exclude"
                },
                {
                  "name": "workspace-only",
                  "type": "boolean",
                  "description": "Only workspace changelog"
                },
                {
                  "name": "per-package",
                  "type": "boolean",
                  "description": "Only per-package changelogs"
