                {
                  "name": "since-tag",
                  "type": "string",
                  "description": "Shorthand for --from <tag>"
                },
                {
                  "name": "format",
                  "type": "string",
                  "default": "both",
                  "description": "Output format",
                  "choices": [
                    "json",
                    "md",
                    "both"
                  ]
                },
                {
                  "name": "level",
                  "type": "string",
                  "default": "standard",
                  "description": "Detail level",
                  "choices": [
